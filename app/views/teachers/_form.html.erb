<%= simple_form_for teacher do |form| %>
  <%= form.input :first_name %>
  <%= form.input :last_name %>
  <%= form.input :phone %>
  <%= form.input :email %>
  <%= form.input :gender, as: :select, collection: [['Female', 'female'], ['Male', 'male']] %>
  <%= form.input :date_of_birth %>
  
  <%= form.button :submit, class: 'btn btn-primary' %>
<% end %>